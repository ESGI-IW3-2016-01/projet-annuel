<!-- Navigation ================================================== -->
<div class="studentBot-background">
    <div class="container">
        <div class="header-container header">
            <a class="navbar-brand logo" href="#"> <img class="logo" src="/images/logo-reseau-ges.png"/> </a>
           {#  <a href="#email-form">
                <button class="header-btn"> Download FREE!</button>
            </a> #}
            <div class="header-right">
                <a class="navbar-item" href="#team">{{ "navbar_team_link" | trans({}, 'messages') }}</a>
                {% if is_granted('ROLE_ADMIN')  %}
                    <a class="navbar-item" href="{{ path('admin') }}">
                        <i class="fa fa-cog" aria-hidden="true"></i>
                        {{ "navbar_admin_link" | trans({}, 'messages') }}
                    </a>
                {% endif %}
                {% if is_granted('ROLE_USER') %}
                    <a class="navbar-item" href="{{ path('fos_user_profile_show') }}">
                        <i class="fa fa-user" aria-hidden="true"></i>
                        {{ "navbar_profile_link" | trans({}, 'messages') }}
                    </a>
                {% endif %}
                {% if app.user %}
                    <a class="navbar-item" href="{{ path('fos_user_security_logout') }}">
                        <i class="fa fa-sign-out" aria-hidden="true"></i>
                        {{ "logout" | trans({}, 'messages') }}
                    </a>
                {% endif %}

                <a class="navbar-item {% if app.request.locale == 'fr'%}active-flag{% endif %}" href="{{ path('choose_language', {'locale' : 'fr'}) }}" role="button">
                    <img class="lang-flag" width="22" height="22" src="/images/flags/fr.png" alt="FR" />
                </a>

                <a class="navbar-item {% if app.request.locale == 'en'%}active-flag{% endif %}" href="{{ path('choose_language', {'locale' : 'en'}) }}" role="button">
                    <img class="lang-flag" width="22" height="22" src="/images/flags/en.png" alt="EN" />
                </a>
            </div>
        </div>

        <!--navigation-->


        <!-- Hero-Section
          ================================================== -->

        <div class="hero row">
            <div class="hero-right col-sm-6 col-sm-6">
                <h1 class="header-headline bold"> {{ "index_h1" | trans({}, 'messages') }}<br></h1>
                <h4 class="header-running-text light"> {{ "simple_answers_speed" | trans({}, 'messages') }}</h4>
                <a href="https://www.messenger.com/t/1860943057468564" target="_blank">
                    <button class="hero-btn">{{ "talk_me" | trans({}, 'messages') }}</button>
                </a>
                    {% if app.user is empty %}
                        <a href="{{ url("hwi_oauth_service_redirect", {service: "facebook"}) }}" class="btn btn-block btn-social btn-facebook" ><span class="fa fa-facebook"></span> {{ "sign_in_fb" | trans({}, 'messages') }}</a>                    
                    {% endif %}
